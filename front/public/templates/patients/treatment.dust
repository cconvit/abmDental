                                     <textarea class="form-control"name="orthodonticsModalDescription" id="orthodonticsModalDescription" rows="6"></textarea>
                                  </div>
                                </div>
                            </p>
                            <p>
                                <div class="col-md-12 form-group">
                                  <label class="col-md-3 control-label">
                                       {@pre type="content" key="orthodontics_modal_type"/}
                                       <span class="required" aria-required="true"> * </span>
                                   </label>
                                  <div class="col-md-9">
                                    <select id="orthodonticsModalType" name="orthodonticsModalType" class="form-control">
                                          <option value="control">{@pre type="content" key="orthodontics_modal_control"/}</option>
                                          <option value="diagnosis">{@pre type="content" key="orthodontics_modal_diagnosis"/}</option>
                                          <option value="assembly">{@pre type="content" key="orthodontics_modal_assembly"/}</option>
                                          <option value="retirement">{@pre type="content" key="orthodontics_modal_retirement"/}</option>
