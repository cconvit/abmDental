                            <p>
                                <div class="col-md-12 form-group">
                                  <label class="col-md-3 control-label">
                                       {@pre type="content" key="orthodontics_modal_title"/}
                                       <span class="required" aria-required="true"> * </span>
                                   </label>
                                  <div class="col-md-9">
                                    <input type="text" class="form-control" id="orthodonticsModalTitle" name="orthodonticsModalTitle" placeholder="{@pre type="content" key="orthodontics_modal_title"/}">
                                  </div>
                                </div>
