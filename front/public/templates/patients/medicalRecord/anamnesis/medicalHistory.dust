<div class="portlet box purple">
  <div class="portlet-title">
    <div class="caption">
      <i class="fa fa-user-md"></i>{@pre type="content" key="medical_history"/}
    </div>
    <div class="tools">
      <a href="javascript:;" class="expand">
      </a>
    </div>
  </div>
  <div id="anamnesis-medicalHistory-portlet" class="portlet-body form" style="display: none;">
   <form id="anamnesis-medicalHistory-form" name="anamnesis-medicalHistory-form" action="/patients/medicalRecord/anamnesis/medicalHistory" class="horizontal-form">
      <input type="hidden" name="identity" value="{id_number}">
      <input type="hidden" name="_csrf" value="{_csrf}">
      <div class="form-body">
        <div class="row">
          <div class="col-md-6">
            <div class="form-group">
              <label class="control-label">{@pre type="content" key="prenatal"/}</label>
              <textarea class="form-control"name="prenatal" id="prenatal" rows="3">{medical_history.prenatal_natal}</textarea>
            </div>
          </div>
          <!--/span-->
          <div class="col-md-6">
            <div class="form-group">
              <label class="control-label">{@pre type="content" key="organ_of_sense"/}</label>
              <textarea class="form-control"name="organOfSense" id="organOfSense" rows="3">{medical_history.organ_of_sense}</textarea>
            </div>
          </div>
          <!--/span-->
        </div>
        <!--/row-->
        <div class="row">
          <div class="col-md-6">
            <div class="form-group">
              <label class="control-label">{@pre type="content" key="childhood_diseases"/}</label>
              <textarea class="form-control"name="childhoodDiseases" id="childhoodDiseases" rows="3">{medical_history.childhood_diseases}</textarea>
            </div>
          </div>
          <!--/span-->
          <div class="col-md-6">
            <div class="form-group">
              <label class="control-label">{@pre type="content" key="psychiatric"/}</label>
                  <textarea class="form-control"name="psychiatric" id="psychiatric" rows="3">{medical_history.psychiatric}</textarea>
            </div>
          </div>
          <!--/span-->
        </div>
        <!--/row-->
        <div class="row">
          <div class="col-md-6">
            <div class="form-group">
              <label class="control-label">{@pre type="content" key="immunizations"/}</label>
              <textarea class="form-control"name="immunizations" id="immunizations" rows="3">{medical_history.immunizations}</textarea>
            </div>
          </div>
          <!--/span-->
          <div class="col-md-6">
            <div class="form-group">
              <label class="control-label">{@pre type="content" key="cardio_vascular_system"/}</label>
              <textarea class="form-control"name="cardioVascularSystem" id="cardioVascularSystem" rows="3">{medical_history.cardio_vascular_system}</textarea>
            </div>
          </div>
          <!--/span-->
        </div>
        <!--/row-->
        <div class="row">
          <div class="col-md-6">
            <div class="form-group">
              <label class="control-label">{@pre type="content" key="surgical"/}</label>
              <textarea class="form-control"name="surgical" id="surgical" rows="3">{medical_history.surgical}</textarea>
            </div>
          </div>
          <!--/span-->
          <div class="col-md-6">
            <div class="form-group">
              <label class="control-label">{@pre type="content" key="endocrine_system"/}</label>
              <textarea class="form-control"name="endocrineSystem" id="endocrineSystem" rows="3">{medical_history.endocrine_system}</textarea>
            </div>
          </div>
          <!--/span-->
        </div>
        <!--/row-->
        <div class="row">
          <div class="col-md-6">
            <div class="form-group">
              <label class="control-label">{@pre type="content" key="traumatic"/}</label>
              <textarea class="form-control"name="traumatic" id="traumatic" rows="3">{medical_history.traumatic}</textarea>
            </div>
          </div>
          <!--/span-->
          <div class="col-md-6">
            <div class="form-group">
              <label class="control-label">{@pre type="content" key="hematopoietic_system"/}</label>
              <textarea class="form-control"name="hematopoieticSystem" id="hematopoieticSystem" rows="3">{medical_history.hematopoietic_system}</textarea>
