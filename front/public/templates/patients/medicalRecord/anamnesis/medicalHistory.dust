          <div class="col-md-6">
              <div class="clearfix">
                <button type="button" name="anamnesis-medicalHistory-button" id="anamnesis-medicalHistory-button" data-msg="{@pre type="content" key="processing"/}" class="btn purple"><i class="fa fa-edit"></i>{@pre type="content" key="save"/}</button>
              </div>
          </div>
        </div>
        <!--/row-->
      </div>
    </form>
  </div>
</div>
