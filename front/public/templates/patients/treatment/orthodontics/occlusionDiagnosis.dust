<div class="portlet box green">
  <div class="portlet-title">
    <div class="caption">
      <i class="fa fa-compress"></i>{@pre type="content" key="occlusion_diagnosis"/}
    </div>
    <div class="tools">
      <a href="javascript:;" class="expand">
      </a>
    </div>
  </div>
  <div id="orthodontic-occlusionDiagnosis-portlet" class="portlet-body form" style="display: none;">
   <form id="orthodontic-occlusionDiagnosis-form" name="orthodontic-occlusionDiagnosis-form" action="/patients/treatment/orthodontic/occlusionDiagnosis/update" method="post" class="horizontal-form">
