          </select>
        </div>
      </div>
      <!--/span-->
      <div class="col-md-6">
        <div class="form-group">
          <label class="control-label">{@pre type="content" key="distance"/}</label>
          <div class="input-group input-small">
             <input id="maxillaryMidlineDistance" type="text" name="maxillaryMidlineDistance" class="input-group input-small" value="{treatment.orthodontic.occlusion_diagnosis.maxillary_dental_distance}">
