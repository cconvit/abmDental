        </div>
      </div>
      <!--/span-->
    </div>
    <!--/row-->
    <div class="row">
      <div class="col-md-6">
        <div class="form-group">
          <label class="control-label">{@pre type="content" key="superior_arch"/}</label>
          <select id="superiorArch" name="superiorArch" class="form-control">
            <option value="ovoid">{@pre type="content" key="ovoid"/}</option>
            <option value="triangular">{@pre type="content" key="triangular"/}</option>
